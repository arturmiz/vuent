{
  "name": "vuent",
  "version": "0.1.0",
  "description": "Vue.js components implementing Microsoft's Fluent Design",
  "author": {
    "name": "Artur Mizera",
    "email": "arturkoduje@outlook.com"
  },
  "license": "MIT",
  "main": "dist/vuent.cjs.js",
  "module": "dist/vuent.esm.js",
  "browser": "dist/vuent.umd.js",
  "homepage": "http://vuentjs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/arturmiz/vuent"
  },
  "bugs": {
    "url": "https://github.com/arturmiz/vuent/issues"
  },
  "scripts": {
    "build": "npm run lint && npm run test && rollup -c",
    "build:ci": "npm run lint && npm run test:ci && rollup -c",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "rollup -c -w",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint --ext .js, .vue .",
    "test": "jest --config=jest.config.json",
    "test:ci": "npm run test:coverage && codecov",
    "test:dev": "jest --config=jest.config.json --watch",
    "test:coverage": "jest --config=jest.config.json --coverage"
  },
  "peerDependencies": {
    "vue": "~2.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "6.0.2",
    "@commitlint/config-conventional": "6.0.2",
    "@vue/test-utils": "^1.0.0-beta.15",
    "babel-core": "~6.26.0",
    "babel-jest": "~22.2.0",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-env": "~1.6.1",
    "codecov": "~3.0.2",
    "eslint": "~4.16.0",
    "eslint-plugin-jest": "~21.7.0",
    "eslint-plugin-vue": "~4.2.0",
    "husky": "~0.14.3",
    "jest": "~22.4.3",
    "jest-serializer-vue": "~1.0.0",
    "rollup": "~0.55.5",
    "rollup-plugin-babel": "~3.0.3",
    "rollup-plugin-commonjs": "~8.3.0",
    "rollup-plugin-node-resolve": "~3.0.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "sass-loader": "~7.0.3",
    "vue": "~2.5.13",
    "vue-jest": "~2.5.0",
    "vue-template-compiler": "~2.5.16",
    "vuepress": "~0.10.1"
  }
}
